{
  "runtime.version": "LuaJIT",
  "runtime.path": [
    "lua/?.lua",
    "lua/?/init.lua"
  ],
  "diagnostics.globals": ["vim",
        "local",
        "global",
        "null_ls",
        "setup_formatter",
        "formatters"],
  "workspace.checkThirdParty": false,
  "workspace.library": [
    "$VIMRUNTIME"
  ],
    "diagnostics.disable": [
        "trailing-space"
    ]
}
